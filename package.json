{
  "name": "nest-user",
  "version": "1.0.0",
  "description": "test nest js",
  "author": "Peter",
  "license": "MIT",
  "scripts": {
    "log": "cross-env NODE_ENV=development DEBUG=api:*",
    "dev": "nodemon",
    "webpack:node": "node dist/server",
    "webpack": "webpack --config webpack.config.js",
    "build": "rimraf dist && tsc",
    "prestart": "npm run build",
    "start": "DEBUG=api:* pm2 start process.json --env production",
    "test": "cross-env NODE_ENV=test nyc mocha --require test/index.ts --opts test/mocha.opts"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/mongoose": "^5.1.0",
    "class-validator": "^0.9.1",
    "dotenv": "^6.0.0",
    "fastify-formbody": "^2.0.0",
    "morgan": "^1.9.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typegoose": "^5.2.1",
    "typescript": "^2.6.2",
    "yarn": "^1.7.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.0.0",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/mongoose": "^5.2.2",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "cross-env": "^5.2.0",
    "jest": "^21.2.1",
    "mongoose": "^5.2.5",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "webpack": "^4.16.2",
    "webpack-cli": "^2.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
